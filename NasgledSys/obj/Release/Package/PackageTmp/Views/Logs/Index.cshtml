@model IEnumerable<NasgledSys.Models.Logs>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

 
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.EventDateTime)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EventLevel)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UserName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MachineName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EventMessage)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ErrorSource)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ErrorClass)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ErrorMethod)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ErrorMessage)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.InnerErrorMessage)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.EventDateTime)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EventLevel)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UserName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MachineName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EventMessage)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ErrorSource)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ErrorClass)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ErrorMethod)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ErrorMessage)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.InnerErrorMessage)
        </td>
        <td>
             
            @Html.ActionLink("Details", "Details", new { id=item.Id }) 
           
        </td>
    </tr>
}

</table>
